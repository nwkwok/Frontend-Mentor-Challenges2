 setPage] = useState('')\n\n\n  useEffect(() => {\n    async function getCountries() {\n      const response = await fetch('https://restcountries.eu/rest/v2/all');\n      const body = await response.json();\n      setCountries(\n        body.map((country) => {\n          return standardize(country);\n        })\n      );\n    }\n    getCountries();\n  }, []);\n\n  // Sets state of country to input value\n  const handleCountryChange = (e) => {\n    setInput(e.target.value);\n  };\n\n  // Sets state of filter, fetches the region of dropdown select value and sets state to an array that holds all countries from that region\n  const handleRegionChange = (e) => {\n    setRegion(e.target.value);\n    setInput('');\n  }\n\n  //Render cards after filtering input value\n  let filteredCountries = countries.filter((country) => {\n    if (region === 'Filter by Region') {\n      return true;\n    }\n    return country.region === region;\n  });\n\n  //Filter country by input state\n  filteredCountries = filteredCountries.filter((country) => {\n    return country.country.toLowerCase().includes(input.toLowerCase());\n  });\n\n  const ListOfCards = ({list}) => {\n\n    return (\n      <>\n        {list.map((item) => {\n\n          return (\n            <Link key={item.country} \n              to={`/${item.country}`} \n              style={{textDecoration: 'none'}}>\n            <Country\n              key={item.country}\n              flag={item.flag}\n              country={item.country}\n              population={item.population}\n              region={item.region}\n              capital={item.capital}\n            />\n            </Link>\n          );\n        })}\n      </>\n    );\n  };\n\n  return (\n    <ThemeContextProvider>\n      <div className={styles.container}>\n      <Navbar />\n      <Switch>\n        <Route exact path=\"/\">\n          <Selection\n            country={input}\n            handleCountryChange={handleCountryChange}\n            region={region}\n            handleRegionChange={handleRegionChange}\n        />\n        <div className={styles.cardContainer}>\n          <ListOfCards \n            list={filteredCountries}\n            />\n        </div>\n        </Route>\n        <Route \n          path='/:countryName' component={CountryPage} />\n      </Switch>\n    </div>\n    </ThemeContextProvider>\n  );\n}\n\n// Helper Functions\nconst standardize = (item) => {\n  return {\n    flag: item.flag,\n    country: item.name,\n    population: item.population,\n    region: item.region,\n    capital: item.capital,\n  };\n};\n\nexport default App;\n",["42","43"],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/Filter/index.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/index.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/Input/index.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/CountryPage/CountryPage.js",["44"],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/Country/index.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/layout/Selection/index.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/context/ThemeContext.js",[],"/Users/nwkwok/Desktop/Software Development/FeMentor/frontend-mentor2/rest-countries-api-with-color-theme-switcher-master/src/components/layout/Navbar/Navbar.js",["45","46"],{"ruleId":"47","severity":1,"message":"48","line":1,"column":38,"nodeType":"49","messageId":"50","endLine":1,"end